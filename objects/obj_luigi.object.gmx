    <event eventtype="3" enumb="2">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Handles Mario's sprites

//Disable ducking for certain powerups
if (global.powerup == cs_frog)
    isduck = 0;

//If Mario is not ducking
if (isduck == 0)
{

    //Select Mask
    if (global.powerup == cs_small
    || global.powerup == cs_crown)
        mask_index = spr_smallmask;
    else
        mask_index = spr_bigmask;

    //Shoot Animation
    if (global.powerup == cs_fire
    || global.powerup == cs_hammer
    || global.powerup == cs_bomb
    || global.powerup == cs_ice
    || global.powerup == cs_super
    || global.powerup == cs_ninja
    || global.powerup == cs_penguin
    || global.powerup == cs_boomerang
    || global.powerup == cs_pyre
    || global.powerup == cs_eyes)
    {
        if (firing &gt; 0)
            firing--;
    }
    else
        firing = 0;

    //Tail Wiggling
    if (global.powerup == cs_leaf
    || global.powerup == cs_tanooki
    || global.powerup == cs_goldtan)
    {
        if (state == 2
        &amp;&amp; stompstyle == 0)
        {
            if (wiggle &gt; 1)
                wiggle--;
            if (wiggle &gt; 12)
                image_index = 3;
            else if (wiggle &gt; 8)
                image_index = 2;
            else if (wiggle &gt; 4)
                image_index = 1;
            else if (wiggle &gt; 1)
                image_index = 2;
            else if (wiggle == 1)
                image_index = 1;
            else
                image_index = 0;
        }
        else
            wiggle = 0;
    }
    else
        wiggle = 0;

    //Not Holding Anything
    if (holding == 0)
    {

        //Standing State
        if (state == 0)
        {

            //Shooting Projectiles
            if (firing &gt; 0)
            {
                image_speed = 0;
                if (firing &lt; 6 
                &amp;&amp; firing &gt; 3)
                    image_index = 1;
                else
                    image_index = 0;
                sprite_index = scr_luigishoot();
            }

            //Default Standing
            else
            {
                image_speed = 0.15
                sprite_index = scr_luigiidle()
            }

        }

        //Walking State
        else if (state == 1)
        {

            //Animate based on horizontal speed
            image_speed = abs(hspeed/6)

            //Frog Mario only walks
            if global.powerup = cs_frog
                sprite_index = scr_luigiwalk()

            //Skid when moving and facing in opposite directions
            else if (direct = 1 and hspeed &lt; -0.001)
            or (direct = -1 and hspeed &gt; 0.001)
            {
                sprite_index = scr_luigiskid()
                if skidnow = 0
                {
                    skidnow = 1
                    audio_play_sound(snd_skid,1,1)
                }
            }

            //Default Walking
            else
            {

                //Run only when P-Meter is full
                if global.pmeter &lt; 6
                    sprite_index = scr_luigiwalk()
                else
                    sprite_index = scr_luigirun()

            }

        }

        //Jumping State
        else if (state == 2)
        {

            //Swimming
            if (isswim == 1)
            {

                //If frog/penguin Mario is not swimming
                if (noisy == 0)
                {
                    sprite_index = scr_luigiswim();
                    image_speed = 0;
                    if (vspeed &lt; 0)
                        image_index = 1;
                    else
                        image_index = 0;
                }

            }

            //Spin Jumping
            else if stompstyle = 1
            {

                //Propeller Mario Flying
                if global.powerup = cs_propeller
                {

                    //Propeller Mario floating down slowly
                    if vspeed &gt; 0
                    and vspeed &lt; 2
                    {
                        image_speed = 0.15
                        sprite_index = spr_luigi_propeller_slow
                    }

                    //Default propeller Mario flying
                    else
                    {
                        image_speed = vspeed/10
                        sprite_index = spr_luigi_propeller_fly
                    }

                }

                //Default Spin Jumping
                else
                {
                    image_speed = 0.5
                    sprite_index = scr_luigispin()
                }

            }

            //Shooting Projectiles
            else if firing &gt; 0
            {
                image_speed = 0
                if firing &lt; 6 and firing &gt; 3
                    image_index = 1
                else
                    image_index = 0

                //Penguin Mario Hotfix
                if global.powerup = cs_penguin
                    sprite_index = spr_luigi_penguin_shoot2
                else
                    sprite_index = scr_luigiswim()

            }

            //Default Jumping
            else
            {

                //Floating Animation
                if global.powerup = cs_carrot
                or global.powerup = cs_bee
                {
                    if isfloating = 1
                        image_speed = 0.15
                    else
                        image_index = 0
                }
                //Squirrel Mario floating
                if global.powerup = cs_squirrel
                and (squirrelpropel = 1 or (vspeed &gt; 0 and (keyboard_check(vk_shift))))
                {
                    sprite_index = spr_luigi_squirrel_float
                    if vspeed &lt; 0
                        image_index = 0
                    else if squirrelpropel = 0
                        image_index = 1
                    else
                        image_speed = 0.15
                }

                //Swooper Mario flapping
                else if global.powerup = cs_swooper
                and powerupfly = 1
                {
                    sprite_index = spr_luigi_swooper_flap
                    if vspeed &gt; 0
                        image_index = 0
                    else
                        image_index = 1
                }

                //Super Crown Mario floating
                else if global.powerup = cs_crown
                and vspeed &gt;= 0
                {
                    if vspeed &gt; 0
                    {
                        sprite_index = spr_px_peach_jump
                        image_index = 0
                    }
                    else
                    {
                        sprite_index = spr_px_peach_jump
                        image_index = 1
                    }
                }

                //Run only when P-Meter is full
                else if global.pmeter &lt; 6
                {
                    sprite_index = scr_luigijump()
                    if vspeed &gt; 0
                    { 
                        if (global.powerup = cs_leaf
                        or global.powerup = cs_tanooki
                        or global.powerup = cs_goldtan)
                            image_index = 1+(wiggle/4)
                        else
                            image_index = 1
                    }
                    else
                    {
                        if (global.powerup = cs_leaf
                        or global.powerup = cs_tanooki
                        or global.powerup = cs_goldtan)
                            image_index = 0+(wiggle/4)
                        else
                            image_index = 0
                    }
                }
                else
                {
                    sprite_index = scr_luigirunjump()
                    if vspeed &gt; 0
                    {
                        if (global.powerup = cs_leaf 
                        or global.powerup = cs_tanooki 
                        or global.powerup = cs_goldtan)
                            image_index = 0+(wiggle/4)
                        else
                            image_index = 1
                    }
                    else
                    {
                        if (global.powerup = cs_leaf 
                        or global.powerup = cs_tanooki 
                        or global.powerup = cs_goldtan)
                            image_index = 0+(wiggle/4)
                        else
                            image_index = 0
                    }
                }

            }

        }

        //Special Swimming Animations
        if (global.powerup == cs_frog
        || global.powerup == cs_penguin)
        {
            if (isswim == 1)
            {
                /*if swimtype = 0
                    sprite_index = scr_luigiswim()
                if swimtype = 1
                    sprite_index = scr_luigiswim2()
                if swimtype = 2
                    sprite_index = scr_luigiswim3()
                if noisy = 1
                    image_speed = 0.15
                else
                    image_speed = 0
                */
                
                            
                    //If Mario is swimming to the sides
                    if (swimtype == 0) {
                                
                        //Set up the sprite
                        sprite_index = scr_luigiswim();
                                
                        //...and animate it
                        if (hspeed == 0)
                            image_speed = 0.1;
                        else
                            image_speed = 0.1+(0.1*keyboard_check(vk_control));
                    }
                                
                    //Otherwise, if Mario is swimming upwards
                    else if (swimtype == 1) {
                                
                        //Set up the sprite
                        sprite_index = scr_luigiswim2();
                                    
                        //Set the speed
                        if ((x != xprevious) || (y != yprevious))
                            image_speed = 0.1+(0.1*keyboard_check(vk_control));
                        else
                            image_speed = 0;
                    }
                                
                    //Otherwise, if Mario is swimming downwards
                    else if (swimtype == 2) {
                                
                        //Set up the sprite
                        sprite_index = scr_luigiswim3();
                                    
                        //Set the speed
                        if ((x != xprevious) || (y != yprevious))
                            image_speed = 0.1+(0.1*keyboard_check(vk_control));
                        else
                            image_speed = 0;
                    }
                            
            }
        }

    }

    //Holding Something
    else
    {

        //Standing State
        if state = 0
        {
            image_speed = 0
            image_index = 0.99
        }

        //Walking State
        if state = 1
            image_speed = abs(hspeed/6)

        //Jumping State
        if state = 2
        {
            image_speed = 0
            if global.powerup = cs_small
                image_index = 2
            else {
                if ((global.powerup == cs_leaf)
                || (global.powerup == cs_tanooki)
                || (global.powerup == cs_goldtan)) 
                    image_index = 0+(wiggle/4);
                else
                    image_index = 3
            }
        }

        //Carrying an item above his head
        if holding = 1 {
            if (state == 2
            &amp;&amp; (global.powerup == cs_leaf
            || global.powerup == cs_tanooki
            || global.powerup == cs_goldtan))
                sprite_index = scr_luigicarry2()
            else
                sprite_index = scr_luigicarry()
        }

        //Holding an item on front of him
        else if holding = 2
            {
                //Usar el sprite de salto si se tienen los siguientes Powerups
                if ((state == 2)
                &amp;&amp; ((global.powerup == cs_leaf)
                || (global.powerup == cs_tanooki)
                || (global.powerup == cs_goldtan)))
                    sprite_index = scr_luigihold2();
                else
                    sprite_index = scr_luigihold();
            }

    }

    //Climbing
    if state = 3
    {
        if speed = 0
            image_speed = 0
        else
            image_speed = 0.15
        sprite_index = scr_luigiclimb()
    }

    //Wall Climbing + Wall Jumping
    else if state = 4
    {

        //Ninja Mario Wall Climbing
        if global.powerup = cs_ninja
        {
            sprite_index = spr_luigi_ninja_wallclimb
            if vspeed = 0
                image_speed = 0
            else
                image_speed = 0.15
        }

        //Wall Jumping
        else
        {
            //If Mario does have the Cat powerup
            if (global.powerup == cs_cat)
            {
                                                
                //Set the alt climbing sprite
                sprite_index = spr_luigi_cat_climb2;
                                                    
                //Animate
                if (vspeed &lt; 0) 
                    image_speed = 0.15;
                else {
                                                    
                    image_speed = 0;
                    image_index = 0;
                }
            }
            else
            {
               sprite_index = scr_luigiwalljump();
            }
            if skidnow = 0
            and global.powerup != cs_squirrel
            {
                skidnow = 1
            }
        }

    }

    //Kicking Animation
    if iamkicking = 1
        sprite_index = scr_luigikick()

}

//If Mario is ducking
else if isduck = 1
{

    //Use Small Mask
    mask_index = spr_smallmask

    //Default Ducking
    sprite_index = scr_luigiduck()
    
    //No animar
    image_speed = 0
    
    //Si se tienen estos poderes
    if ((global.powerup = cs_leaf) 
    || (global.powerup = cs_tanooki)
    || (global.powerup = cs_goldtan)) {
    
        //Establecer el frame correcto
        if (holding = 0)
            image_index = 0+(wiggle/4);
        else if (holding == 2)
            image_index = 5+(wiggle/4);
        else
            image_index = 4
        
    }
    else {
        //Establecer el frame correcto
        if (holding = 0)
            image_index = 0;
        else {
    
            if (holding == 2)
                image_index = 2;
            else
