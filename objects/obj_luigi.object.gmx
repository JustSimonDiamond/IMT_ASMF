        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Create Mario's skid smoke
if (global.pmeter &gt; 5 
&amp;&amp; state == 1 
&amp;&amp; enable_gravity == 1
&amp;&amp; (sprite_index == scr_luigirun()
|| sprite_index == scr_luigihold()
|| sprite_index == scr_luigiwalk()
|| sprite_index == scr_luigiride()))
{
    var smk_dir;
    if (direct == 1)
        smk_dir = -12;
    else
        smk_dir = 12;
            
    with (instance_create(x+smk_dir,y+28,obj_smoke))
    {
        sprite_index = spr_runsmoke;
        image_speed = 0.20;
        depth = -6;
    }
    alarm[5] = 8;
}

else if ((walljump == 1) &amp;&amp; (vspeed &gt; 0)) 
{

    //If Mario is small
    if (global.powerup == cs_small) 
    {
        with (instance_create(x+5*sign(-direct), y+8, obj_smoke))
            sprite_index = spr_smoke_c;
    }
    
    //Otherwise, if Mario is not small
    else 
    {
        with (instance_create(x+5*sign(-direct), y, obj_smoke))
            sprite_index = spr_smoke_c;        
    }
    
    //Repeat the process
    alarm[5] = 4;
}

else
{

    if (skidnow == 1)
    {
        //If Mario is wall jumping, reverse where the smoke appears
        if ((direct = 1 &amp;&amp; state != 4)
        || (direct = -1 &amp;&amp; state = 4))
        {
            with (instance_create(x+1,y+24,obj_smoke))
            {
                sprite_index = spr_skidsmoke;
                depth = -6;
            }
        }
        else
        {
            with (instance_create(x-8,y+24,obj_smoke))
            {
                sprite_index = spr_skidsmoke;
                depth = -6;
            }
        }

    }
    alarm[5] = 8;
}
</string>
